{
  "manifest_version": 2,
  "name": "TAFASU Notify",
  "description": "TAFASU Notification for Who addict TAFASU",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "/img/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "/img/app-16.png",
    "48": "/img/app-48.png",
    "128": "/img/app-128.png"
  },
  "permissions": [
    "background",
    "notifications",
    "https://*.tafasu.com/"
  ]
}
